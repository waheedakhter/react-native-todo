var _curry2 = require("./internal/_curry2");

var _dispatchable = require("./internal/_dispatchable");

var _xall = require("./internal/_xall");

var all = _curry2(_dispatchable(['all'], _xall, function all(fn, list) {
  var idx = 0;

  while (idx < list.length) {
    if (!fn(list[idx])) {
      return false;
    }

    idx += 1;
  }

  return true;
}));

module.exports = all;
